    double num;

            Console.WriteLine("Estrutura de Controle");
            Console.WriteLine("Entre com um número");
            num = double.Parse(Console.ReadLine());
            Console.WriteLine("O número digitado foi: " + num);
            Console.WriteLine("O número digitado é múltiplo de 5???");

            //multiplo de 5, e menor do que 100
            if (num % 5 == 0 && num < 100)
            {
                Console.WriteLine(num + " é múltiplo de 5, ou menor que 100");
            }
            else
            {
                Console.WriteLine(num + " não é múltiplo de 5, ou menor do que 100");
            }

            
